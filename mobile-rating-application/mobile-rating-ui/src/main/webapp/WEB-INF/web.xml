<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">

    <display-name>mobile-rating-application</display-name>

    <servlet>
        <servlet-name>MobileRankerServlet</servlet-name>
        <servlet-class>com.valtech.traning.ratingui.MobileRankerServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>MobileRankerServlet</servlet-name>
        <url-pattern>/MobileRankerServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
